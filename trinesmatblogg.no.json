              },
              "if_statements": []
            }
          ],
          "if_statements": []
        },
        {
          "name": "quantity",
          "data_fetchers": [
            {
              "path": [
                "ingredient",
                "td",
                0,
                "#text"
              ],
              "regex": {
                "expression": "\\d+"
              },
              "if_statements": []
            }
          ],
          "if_statements": []
        },
        {
          "name": "unit",
          "data_fetchers": [
            {
              "path": [
                "ingredient",
                "td",
                0,
                "#text"
              ],
              "regex": {
                "expression": "(\\w+)",
                "group": 1
              },
              "if_statements": []
            }
          ],
          "if_statements": [],
